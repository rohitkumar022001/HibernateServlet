<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.digit.servletHibernate.bean.AadharDetails" table="aadhar">
		<id name="id" column="id"></id>
		<property name="name">
			<column name="name" />
		</property>
		<property name="dob">
			<column name="dob" />
		</property>
		<property name="address">
			<column name="address" />
		</property>
	</class>
</hibernate-mapping>